<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Condition="$(SonarImported) != 'true' AND $(RunSonarAnalysis) == 'true' ">
		<SonarTargetPath Condition="$(SonarTargetPath) == ''">$(MSBuildExtensionsPath)\SonarQube</SonarTargetPath>
    <SonarTargets>$(SonarTargetPath)\Sonar.Integration.v0.1.targets</SonarTargets>
  </PropertyGroup>
  <Import Condition="$(RunSonarAnalysis) == 'true' AND $(SonarImported) != 'true' AND Exists('$(SonarTargets)')" Project="$(SonarTargets)" />
</Project>
